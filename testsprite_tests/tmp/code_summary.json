{
  "tech_stack": [
    "TypeScript",
    "React 19.2.0",
    "Vite",
    "React Router DOM 7.11.0",
    "Supabase (PostgreSQL + Auth)",
    "TanStack React Query 5.90.16",
    "React Hook Form 7.69.0",
    "Zod 4.3.2",
    "Tailwind CSS 4.1.18",
    "Lucide React Icons 0.562.0",
    "Paystack Payment",
    "Sonner Toast Notifications"
  ],
  "features": [
    {
      "name": "Authentication System",
      "description": "User authentication with role-based access control (Student/Lecturer). Supports login, signup, password reset, and session management via Supabase Auth.",
      "files": [
        "src/contexts/AuthContext.tsx",
        "src/components/Login.tsx",
        "src/components/ProtectedRoute.tsx",
        "src/lib/supabase.ts"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {
          "title": "Authentication API",
          "version": "1.0.0"
        },
        "paths": {
          "/auth/login": {
            "post": {
              "summary": "User login",
              "requestBody": {
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "email": {"type": "string", "format": "email"},
                        "password": {"type": "string", "minLength": 6}
                      },
                      "required": ["email", "password"]
                    }
                  }
                }
              },
              "responses": {
                "200": {"description": "Login successful"},
                "401": {"description": "Invalid credentials"}
              }
            }
          },
          "/auth/signup": {
            "post": {
              "summary": "User registration",
              "requestBody": {
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "email": {"type": "string", "format": "email"},
                        "password": {"type": "string", "minLength": 6},
                        "fullName": {"type": "string"},
                        "role": {"type": "string", "enum": ["student", "lecturer"]}
                      },
                      "required": ["email", "password", "fullName", "role"]
                    }
                  }
                }
              }
            }
          },
          "/auth/reset-password": {
            "post": {
              "summary": "Request password reset",
              "requestBody": {
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "email": {"type": "string", "format": "email"}
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "Student Dashboard",
      "description": "Central hub for students displaying academic overview, CGPA, enrolled courses, assignments, announcements, and quick actions.",
      "files": [
        "src/components/StudentDashboard.tsx",
        "src/hooks/useSupabase.ts"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "paths": {
          "/api/student/dashboard": {
            "get": {
              "summary": "Get student dashboard data",
              "parameters": [
                {"name": "student_id", "in": "query", "required": true, "schema": {"type": "string"}}
              ],
              "responses": {
                "200": {
                  "description": "Dashboard data",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "object",
                        "properties": {
                          "gpa": {"type": "number"},
                          "totalCredits": {"type": "number"},
                          "assignments": {"type": "array"},
                          "announcements": {"type": "array"}
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "Course Registration",
      "description": "Student course registration system with credit limit validation, course search, and enrollment management.",
      "files": [
        "src/components/StudentRegistration.tsx"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "paths": {
          "/api/courses/catalog": {
            "get": {
              "summary": "Get available courses",
              "responses": {
                "200": {
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "code": {"type": "string"},
                            "title": {"type": "string"},
                            "units": {"type": "number"},
                            "lecturer": {"type": "string"},
                            "time": {"type": "string"},
                            "seats": {"type": "number"}
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "/api/enrollment/register": {
            "post": {
              "summary": "Register for courses",
              "requestBody": {
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "student_id": {"type": "string"},
                        "course_ids": {"type": "array", "items": {"type": "string"}}
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
